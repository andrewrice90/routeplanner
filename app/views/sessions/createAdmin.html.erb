<center><h1>Admin Sign in</h1>
<%= form_tag admincreate_path do %>
    <table>   
     <tr>  
          <td><%= label_tag :email %></td>
            <td><%= text_field_tag :email %>
        </tr>
        <tr>  
          <td><%= label_tag :password %></td> 
           <td><%= password_field_tag :password %>
        </tr>   
     <tr>  
	<td> 
           <div id ="button"><%= submit_tag "Sign in" %></div>
	</td>
        </tr>
   </table>
   		   <br><br><br>
<% end %>
